<h2 class="text-center page-header">{{model.name}}</h2>


<div class="row">
  <div class="col-md-3 col-md-offset-1">
    <h2> Food Carts: </h2>
    <ul class="list-unstyled">
      {{#each cart in model.carts}}
        <li>{{cart.name}} </li>
      {{/each}}
    </ul>

  </div>


  <div class="col-md-5 col-md-offset-1">
    {{google-maps address=model.address}}
  </div>

</div>


{{#link-to "new-cart"}}<p class="btn btn-success"> Add New Cart</p>{{/link-to}}


{{outlet "new-cart"}}
